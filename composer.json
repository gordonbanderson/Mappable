{
	"name": "weboftalent/mappable",
	"description": "Fork of Uncle Cheese's original Mappable module.  Inline JavaScript is minimal, and templated such that other mapping systems can be added later.  A map editing interface is also included, addable by a one line configuration call.",
	"type": "silverstripe-module",
	"keywords": ["silverstripe", "google maps", "maps", "mappable"],
	"authors": [
		{
			"name": "Gordon Anderson",
			"email": "gordon.b.anderson@gmail.com",
			"homepage": "https://github.com/gordonbanderson/Mappable",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.6.0",
		"silverstripe/framework": "^4@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"squizlabs/php_codesniffer": "^2.3"
	},
	"autoload": {
		"psr-4": {
			"WebOfTalent\\Mappable\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"WebOfTalent\\Mappable\\": "tests"
		}
	},

	"extra": {
		"installer-name": "mappable",
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		},
		"expose": [
			"admin/client/dist",
			"client/dist"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,

	"suggest": {
		"weboftalent/mappable-poi": "1.*"
	},
    "support": {
        "issues": "https://github.com/gordonbanderson/Mappable/issues"
    },
	"license": "BSD-3-Clause"
}
